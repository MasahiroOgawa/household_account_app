{
  "sources": {
    "orico": {
      "name": "Orico",
      "filename": ["*orico*.csv", "*オリコ*.csv"],
      "columns": {
        "date": 0,
        "description": 1,
        "amount": 2,
        "type": -1,
        "balance": -1,
        "accountNumber": -1
      },
      "encoding": "shift-jis",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D"
    },
    "kal": {
      "name": "KAL Bank",
      "filename": ["*kal*.csv", "*カル*.csv"],
      "columns": {
        "date": 0,
        "description": 1,
        "withdrawal": 2,
        "deposit": 3,
        "balance": 4,
        "accountNumber": -1
      },
      "amountColumns": {
        "withdrawal": 2,
        "deposit": 3
      },
      "encoding": "shift-jis",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D"
    },
    "paypay": {
      "name": "PayPay",
      "filename": ["*paypay*.csv", "*ペイペイ*.csv"],
      "columns": {
        "date": 1,
        "description": 2,
        "amount": 3,
        "type": -1,
        "balance": -1,
        "accountNumber": -1
      },
      "encoding": "utf-8",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D H:mm"
    },
    "ufj": {
      "name": "UFJ Bank",
      "filename": ["*ufj*.csv", "*三菱*.csv", "*UFJ*.csv"],
      "columns": {
        "date": 0,
        "description": 1,
        "withdrawal": 4,
        "deposit": 3,
        "balance": 5,
        "notes": 2,
        "accountNumber": -1
      },
      "amountColumns": {
        "withdrawal": 4,
        "deposit": 3
      },
      "encoding": "shift-jis",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D",
      "accountNumberExtraction": true
    },
    "jre": {
      "name": "JRE Bank",
      "filename": ["*jre*.csv", "*JRE*.csv"],
      "columns": {
        "date": 0,
        "withdrawal": 1,
        "deposit": 2,
        "balance": 3,
        "description": 4,
        "accountNumber": -1
      },
      "amountColumns": {
        "withdrawal": 1,
        "deposit": 2
      },
      "encoding": "shift-jis",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D"
    },
    "smbc": {
      "name": "SMBC Bank",
      "filename": ["*smbc*.csv", "*三井住友*.csv", "*SMBC*.csv"],
      "columns": {
        "date": 0,
        "withdrawal": 1,
        "deposit": 2,
        "description": 3,
        "balance": 4,
        "accountNumber": -1
      },
      "amountColumns": {
        "withdrawal": 1,
        "deposit": 2
      },
      "encoding": "shift-jis",
      "skipRows": 1,
      "dateFormat": "YYYY/M/D"
    }
  },
  "customMappings": {},
